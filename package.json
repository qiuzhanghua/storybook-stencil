{
  "name": "storybook-stencil",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/storybook-stencil/storybook-stencil.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@stencil/core": "^4.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@storybook/addon-essentials": "^7.0.25",
    "@storybook/addon-interactions": "^7.0.25",
    "@storybook/addon-links": "^7.0.25",
    "@storybook/blocks": "^7.0.25",
    "@storybook/html": "^7.0.25",
    "@storybook/html-webpack5": "^7.0.25",
    "@storybook/testing-library": "^0.2.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.3",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "puppeteer": "^20.7.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.25"
  },
  "license": "MIT"
}
